            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.apache.jmeter.services.FileServer;
rstr = prev.getResponseDataAsString();

//ss[0]:cpu ss[1]:memory
ss=rstr.split(&quot;\\s+&quot;);
fs = FileServer.getFileServer();
t=new Date().getTime();
elps0=Math.round(Double.valueOf(ss[0])*1000);
elps1=Math.round(Double.valueOf(ss[1])*1000);
fs.write(&quot;logfile&quot;,t+&quot;,&quot;+elps0+&quot;,&quot;+vars.get(&quot;host&quot;)+&quot; Cpu,,,,,true,,0,0,0,0,null,0,0,0\n&quot;);
fs.write(&quot;logfile&quot;,t+&quot;,&quot;+elps1+&quot;,&quot;+vars.get(&quot;host&quot;)+&quot; Memory,,,,,true,,0,0,0,0,null,0,0,0\n&quot;);</stringProp>
