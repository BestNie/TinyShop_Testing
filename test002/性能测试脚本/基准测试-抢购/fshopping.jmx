          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.apache.jmeter.services.FileServer;
//确定文件的绝对路径
jtl = vars.get(&quot;ssh_jtlfile&quot;);
if(jtl.startsWith(&quot;~/&quot;))
	file = FileServer.getFileServer().getBaseDir()+&quot;/&quot;+jtl.substring(2);
else 
	file = new File(jtl).getCanonicalPath();
//文件的绝对路径
file=file.replaceAll(&quot;\\\\&quot;,&quot;/&quot;);
lsprt = file.lastIndexOf(&quot;/&quot;);
//除掉文件名后的路径
dir = file.substring(0, lsprt);
//如果路径不存在，则创建
File dirf=new File(dir);
if(!dirf.exists()) 
	dirf.mkdirs();
//清除文件内容（如果文件已经存在）
fo=new FileOutputStream(file);
fo.close();
//使用JMeter的文件服务来操作文件
fs = FileServer.getFileServer();
fs.reserveFile(file, &quot;iso-8859-1&quot;, &quot;logfile&quot;);
fs.write(&quot;logfile&quot;,&quot;timeStamp,elapsed,label,responseCode,responseMessage,threadName,dataType,success,failureMessage,bytes,sentBytes,grpThreads,allThreads,URL,Latency,IdleTime,Connect\n&quot;);
</stringProp>
            <stringProp name="scriptLanguage">java</stringProp>
          </JSR223Sampler>
          <hashTree/>
        </hashTree>
